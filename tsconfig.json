{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "ESNext"],
      "allowJs": false,
      "skipLibCheck": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react-jsx",
      "incremental": true,
      "noImplicitThis": true
    },
    "include": ["src/**/*", "src/declaration.d.ts"],
    
  }